        "title": "HTTP Requests by Method",
        "type": "timeseries",
        "targets": [
          {
            "expr": "netview_http_requests_total",
            "refId": "A",
            "legendFormat": "{{method}} {{path}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
